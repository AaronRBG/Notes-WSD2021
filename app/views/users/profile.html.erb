<p id="notice"><%= notice %></p>
<html class="default_backround">
    <head>
        <title>Mi Perfil</title>
        
    </head>
    <body>
        <div class="header">
            <h3 class="user-name">Usuario</h3>
            <img class="avatar" src="https://jesusmaestro.escuelateresiana.com/wp-content/uploads/2015/03/user-default.png">
           
        </div>
        <ul class="socials">
            <li>
                <a href="email">email</a>
            </li>
            <li>
                <a href="facebook">facebook</a>
            </li>
            <li>
                <a href="twitter">twitter</a>
            </li>
        </ul>
       <tbody class="notes_table">
    <% @notes.each do |note| %>
      <tr>
        <!--<td><%= note._id %></td> -->
        <td><h4><%= note.title %></h4></td>
        <!--td><%= note.text %></td-->
        <!-- <td><%= note.image %></td> -->
        <td><ul>
          <li>
            <%= link_to 'Show', note_path(note, :_id => note._id) %>
          </li>
          <li>
            <%= link_to 'Edit', edit_note_path(note, :_id => note._id) %>
          </li>
          <li>
            <%= link_to 'Destroy', note_path(note, :_id => note._id), remote: true, method: :delete, data: { confirm: 'Are you sure?' } %>
          </li>
        </ul></td>
       
      </tr>
    <% end %>
  </tbody>
    </body>
</html>














